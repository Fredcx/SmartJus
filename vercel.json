{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "includeFiles": "node_modules/{puppeteer-extra-plugin-*,fs-extra,universalify,graceful-fs}/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}